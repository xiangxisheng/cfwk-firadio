generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model mails {
  id          Int     @id @default(autoincrement())
  created     Int
  domain_from String
  domain_to   String
  email_from  String
  email_to    String
  headers     String
  subject     String
  body_text   String?
  body_html   String?
}

model users {
  user_id  Int    @id @default(autoincrement())
  created  Int
  email    String @unique
  password String
  roles    String
}

model pre_system_menus {
  id        Int     @id @default(autoincrement())
  created   Int
  updated   Int?
  status    Int
  parent_id Int
  type      Int
  name      String
  title     String
  path      String?
  icon      String?
  component String?
  orderNo   Int
  keepalive Int
  redirect  String?
  meta      String?

  @@unique([parent_id, name])
}
